[user]
	email = wbhorn@alaska.edu
	name = William Horn
[default]
	editor = nvim
[core]
	editor = nvim
[alias]
	s = status
    plog = log --oneline --graph
    pl = log --oneline --graph
    ca = !git add --all && git commit
    b = branch
    co = checkout
    r = reset
    plom = pull origin master
    psom = push origin master
[credential]
	helper = cache --timeout=360000
